<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FMSPuntuacion.Vistas.ResultadoDetailPage"
             Title="{Binding Title}">
    
    <StackLayout Spacing="20" Padding="15">
        <Label Text="{Binding Resultado.ganador}" FontSize="Medium" />
        <Label Text="{Binding Resultado.sitio}" FontSize="Small" />
        <Label Text="{Binding Resultado.fecha}" FontSize="Small" />
        <Label Text="{Binding Resultado.player1}" FontSize="Small" />
        <Label Text="{Binding Resultado.player2}" FontSize="Small" />

    
    </StackLayout>
    
</ContentPage>